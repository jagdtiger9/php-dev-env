;;; Do not edit this file, it's just an example
;;; Copy it to .env and set proper options

COMPOSE_PROJECT_NAME=mp

;;;;; Virtual hosts settings
; path to virtual hosts dir; every sub-dir is a single host - magipro etc
HOSTS=../../data/hosts
WEB=../../data/web

; Services data dirs
DATA_XDEBUG=../../data/xdebug
DATA_MYSQL=../../data/mysql
DATA_TARANTOOL=../../data/tarantool
DATA_REDIS=../../data/redis
DATA_SPHINX=../../data/sphinxsearch
LOG_DIR=../../data/log

;;;;; Databases settings
MYSQL_ROOT_PASSWORD=ubuntu34
TARANTOOL_USER_NAME=admin
TARANTOOL_USER_PASS=ubuntu34
MYSQL_REPL_USER=repl
MYSQL_REPL_PASS=repl

;;;;; PHP
DEFAULT_TIME_ZONE=Europe/Moscow
; PHP version: 7.0, 7.1, 7.2 ...
PHP_VERSION=7.3
; set 0 to disable xdebug
ALLOW_XDEBUG=1
XDEBUG_PORT=9001
FPM_LOG_LEVEL=warning
MERCURE_KEY=_secretPushKey_
